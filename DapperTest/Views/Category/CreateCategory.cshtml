@model DapperTest.Dtos.CategoryDtos.CreateCategoryDto
@{
    ViewData["Title"] = "CreateCategory";
}
<div class="container">
    <div class="row">
        <div class="col-6">
            <h1>Kategori Oluştur</h1>
            <hr />
            <form method="post">
                <div class="form-group my-3">
                    <label>Kategori Adı</label>
                    <input type="text" asp-for="CategoryName" class="form-control" />
                </div>
                <div class="form-group my-3">
                    <button class="btn btn-info">Oluştur</button>
                </div>
            </form>
        </div>
    </div>
</div>



